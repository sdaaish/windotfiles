[submodule ".emacs.d"]
	path = .emacs.d
	url = https://github.com/plexus/chemacs2.git
	branch = main
[submodule ".emacs.default"]
	path = .config/emacs.default
	url = https://github.com/sdaaish/emacs.d.git
	branch = master
	ignore = dirty
[submodule ".emacs.doom"]
	path = .config/emacs.doom
	url = https://github.com/hlissner/doom-emacs
	branch = master
	ignore = dirty
[submodule ".config/crafted-emacs"]
	path = .config/crafted-emacs
	url = https://github.com/SystemCrafters/crafted-emacs.git
	branch = master
	ignore = dirty
